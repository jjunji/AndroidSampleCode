<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tenmanager_1.Fragment.FindContactFragment.ContactFragment">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.example.tenmanager_1.ContactUtil.IndexableListView
            android:id="@+id/customerListView"
            android:layout_width="match_parent"
            android:layout_height="440dp"
            android:layout_alignParentTop="true"
            android:layout_above="@+id/btnComfirm" />

        <Button
            android:id="@+id/btnComfirm"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:background="@color/buttonColor"
            android:text="완 료"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    </RelativeLayout>

</FrameLayout>
